{
    "categories": [
      {
        "title": "## 🚀 Features",
        "labels": ["feature", "type: feature", "type: enhancement"]
      },
      {
        "title": "## 🐛 Fixes",
        "labels": ["fix", "type: bug"]
      },
      {
        "title": "## 🧪 Tests",
        "labels": ["test"]
      }
    ],
    "ignore_labels": [
      "ignore"
    ],
    "sort": "DESC",
    "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
    "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
    "empty_template": "- no changes"
  }